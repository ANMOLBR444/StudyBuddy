<form action="/notes" method="POST">
    <label for="sem">Sem: </label>
    <input type="number" name="note[semester]" id="sem" class="form-control" placeholder="Enter Semester" required>
    <label for="subject">Sub: </label>
    <select name="note[subject]" class="form-select" id="subject"aria-label="Default select example">
        <option selected>Open this select menu</option>
        <option value="Parallel and Distributed Computing">Parallel and Distributed Computing</option>
        <option value="Gender Studies">Gender Studies</option>
        <option value="Compiler Design">Compiler Design</option>
        <option value="Software Engineering and Agile Development">Software Engineering and Agile Development</option>
        <option value="Generative AI">Generative AI</option>
        <option value="Data Privacy and Security">Data Privacy and Security</option>
    </select>
    <label for="topic">topic</label>
    <input type="text" name="note[topic]" id="topic" class="form-control" placeholder="Enter Topic" required>
    <label for="file">file URL</label>
    <input type="text" name="note[fileurl]" id="file" class="form-control" required>
    <button>submit</button>
</form>